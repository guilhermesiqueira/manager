      "prettier --check"
    ],
    "*.js": [
      "eslint",
      "prettier --check"
    ],
    "*.tsx": [
      "eslint",
      "prettier --check"
    ],
    "styles.ts": [
      "stylelint --fix",
      "stylelint"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(gapi-script)/)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.stories.{ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!<rootDir>/src/config/routes/ComponentLoader.tsx",
      "!<rootDir>/src/i18n.ts",
      "!<rootDir>/src/reportWebVitals.ts",
      "!<rootDir>/src/index.tsx"
    ]
  },
  "browser": {
    "crypto": false,
    "fs": false,
    "path": false,
    "os": false,
    "net": false,
    "stream": false,
    "tls": false
